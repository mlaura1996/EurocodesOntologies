<owl:AllDisjointClasses>
  <owl:members rdf:parseType="Collection">
    <rdf:Description rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#Water"/>
    <rdf:Description rdf:about="#AdmixtureMaterial"/>
    <rdf:Description rdf:about="#AggregateMaterial"/>
    <rdf:Description rdf:about="#BinderMaterial"/>
  </owl:members>
