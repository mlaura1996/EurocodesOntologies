@prefix bot: <https://w3id.org/bot#> .
@prefix cc: <http://creativecommons.org/ns#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ec: <http://www.w3id.org/eurocodes/ec1990#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix qudt: <http://qudt.org/3.1.2/schema/qudt/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ec: a owl:Ontology ;
    cc:license "http://creativecommons.org/licenses/by/3.0/" ;
    dcterms:contributor "Agnieszka Jędrzejewska (Silesian Unviersity of Technology)",
        "Carlos Ramonell (Politechnic University of Catalonia)",
        "Maria Laura Leonardi (University of Minho)" ;
    dcterms:creator "Carlos Ramonell Cazador (carlos.ramonell@upc.edu), Agnieszka Jędrzejewska (agnieszka.jedrzejewska@polsl.pl), Maria Laura Leonardi (mlauraleonardi@gmail.com), José Granja (granja@civil.uminho.pt), Rolando Chacón (rolando.chacon@upc.edu), Daniel V. Oliveira (danvco@civil.uminho.pt), Miguel Azenha (miguel.azenha@civil.uminho.pt)" ;
    dcterms:date "2025-06-30"^^xsd:date ;
    dcterms:description "Core ontology formalising knowledge from Eurocode 0 for automated structural design and verification" ;
    dcterms:format "ttl" ;
    dcterms:language "en" ;
    dcterms:modified "2025-06-30"^^xsd:date ;
    dcterms:title "ECO - Eurocode Core Ontology" ;
    vann:preferredNamespacePrefix "ec" ;
    vann:preferredNamespaceUri "http://www.w3id.org/eurocodes/ec1990#" ;
    owl:imports <https://www.w3id.org/bot> ;
    owl:versionInfo "1.0.0" ;
    skos:hasTopConcept ec:Action,
        ec:CombinationOfActions,
        ec:ConstructionWork,
        ec:DesignSituation,
        ec:EffectOfAction,
        ec:LimitState .

ec:AccelerationEffect a owl:Class ;
    rdfs:label "Acceleration effect"@en ;
    dcterms:source "EN 1990:2002, Section 4.1.5(2)" ;
    rdfs:subClassOf ec:DynamicEffect ;
    skos:definition "Effect involving acceleration of the structure or structural members." .

ec:AccessibleRoofs a owl:Class ;
    rdfs:label "Category I - accessible roofs"@en ;
    dcterms:source "EN 1991-1-1:2002 Table 6.9" ;
    rdfs:subClassOf ec:EurocodeZone ;
    skos:definition "Roof accessible with occupancy according to categories A to G" ;
    skos:example "Roof used as terraces, gardens, or other occupied spaces" .

ec:AgriculturalStructure a owl:NamedIndividual,
        ec:Structure ;
    rdfs:label "Agricultural structure"@en ;
    dcterms:source "EN 1990:2002, Table 2.1" ;
    skos:definition "A building or construction primarily designed and used for farming-related activities." ;
    skos:example "barn, greenhouse, animal shelter" .

ec:AreaWithTables a owl:Class ;
    rdfs:label "Category C1 - areas with tables"@en ;
    dcterms:source "EN 1991-1-1:2002 Table 6.1" ;
    rdfs:subClassOf ec:CongregationArea ;
    skos:definition "Area with tables" ;
    skos:example "areas in schools, cafés, restaurants, dining halls, reading rooms, receptions" .

ec:AreasWithFixedSeats a owl:Class ;
    rdfs:label "Category C2 - areas with fixed seats"@en ;
    dcterms:source "EN 1991-1-1:2002 Table 6.1" ;
    rdfs:subClassOf ec:CongregationArea ;
    skos:definition "Area with fixed seats" ;
    skos:example "areas in churches, theatres or cinemas, conference rooms, lecture halls, assembly halls, waiting rooms, railway waiting rooms" .

ec:AreasWithoutObstacle a owl:Class ;
    rdfs:label "Category C3 - areas without obstacles"@en ;
    dcterms:source "EN 1991-1-1:2002 Table 6.1" ;
    rdfs:subClassOf ec:CongregationArea ;
    skos:definition "Area without obstacles for moving people" ;
    skos:example "areas in museums, exhibition rooms, etc. and access areas in public and administration buildings, hotels, hospitals, railway station forecourts" .

ec:AxialForce a owl:Class ;
    rdfs:label "Axial force"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.3.2" ;
    rdfs:subClassOf ec:InternalForce ;
    skos:altLabel "N" ;
    skos:definition "Internal force acting along the axis of structural members." .

ec:BendingMoment a owl:Class ;
    rdfs:label "Bending moment"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.3.2" ;
    rdfs:subClassOf ec:InternalForce ;
    skos:altLabel "M" ;
    skos:definition "Internal moment causing bending in structural members." .

ec:BendingMomentResistance a owl:Class ;
    rdfs:label "Bending moment resistance"@en ;
    rdfs:subClassOf ec:Resistance ;
    skos:altLabel "M_Rd" ;
    skos:definition "Design value of the maximum bending moment that a structural cross-section can safely resist without failure." .

ec:BridgeStructure a owl:NamedIndividual,
        ec:Structure ;
    rdfs:label "Bridge structure"@en ;
    dcterms:source "EN 1990:2002, Table 2.1" ;
    skos:definition "Engineered construction designed to span physical obstacles such as rivers, valleys, roads, or railways, providing a safe passage for vehicles, pedestrians, or utilities." ;
    skos:example "beam bridge, arch bridge, suspension bridge" .

ec:BuildingStructure a owl:NamedIndividual,
        ec:Structure ;
    rdfs:label "Building structure"@en ;
    dcterms:source "EN 1990:2002, Table 2.1" ;
    skos:definition "Load-bearing framework or system designed to support and transfer all applied loads safely to the foundation and ultimately to the ground." ;
    skos:example "masonry building, steel frame building, timber-framed house" .

ec:CharacteristicCombination a owl:Class ;
    rdfs:label "Characteristic combination"@en ;
    dcterms:source "EN 1990:2002, Section 6.5.3(2)a" ;
    rdfs:subClassOf ec:SLSCombination ;
    owl:disjointWith ec:FrequentCombination,
        ec:QuasiPermanentCombination ;
    skos:definition "Serviceability combination normally used for irreversible limit states." ;
    skos:scopeNote "Used for irreversible serviceability limit state verifications" .

ec:CivilEngineeringStructure a owl:NamedIndividual,
        ec:Structure ;
    rdfs:label "Civil engineering structure"@en ;
    dcterms:source "EN 1990:2002, Table 2.1" ;
    skos:definition "Constructed system composed of interconnected physical elements designed, analyzed, and built to withstand environmental and operational loads, enabling the provision of essential services such as transport, shelter, water management, or energy distribution." ;
    skos:example "dam, tunnel, airport, retaining wall, power station, road" .

ec:CivilEngineeringWork a owl:Class ;
    rdfs:label "Civil engineering work"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.1.2" ;
    rdfs:subClassOf ec:ConstructionWork ;
    skos:definition "Type of construction work for civil engineering purposes such as bridges, retaining walls, etc." ;
    skos:example "bridge, retaining wall, tunnel" .

ec:CompressiveStrength a owl:Class ;
    rdfs:label "Compressive strength"@en ;
    rdfs:subClassOf ec:MaterialProperty ;
    skos:definition "Compressive strength of a material." .

ec:Concrete a owl:Class ;
    rdfs:label "Concrete"@en ;
    rdfs:subClassOf ec:Material ;
    skos:definition "A composite material consisting of a mixture of cement, water, aggregates (coarse and fine), and, where appropriate, admixtures and additions, which develops its strength by hydration of the cement. " .

ec:CreepEffect a owl:Class ;
    rdfs:label "Creep effect"@en ;
    rdfs:subClassOf ec:TimeDependentEffect ;
    skos:definition "Long-term deformation effect due to sustained loading." .

ec:DeadLoad a owl:Class ;
    rdfs:label "Dead load"@en ;
    dcterms:source "EN 1991-1-1:2002" ;
    rdfs:subClassOf ec:PermanentAction ;
    skos:definition "." .

ec:Deflection a owl:Class ;
    rdfs:label "Deflection"@en ;
    dcterms:source "EN 1990:2002, Section 1.6" ;
    rdfs:subClassOf ec:LinearDeformation ;
    skos:altLabel "w" ;
    skos:definition "Vertical deflction of a structural member." .

ec:Density a owl:Class ;
    rdfs:label "Density"@en ;
    rdfs:subClassOf ec:MaterialProperty ;
    skos:definition "Mass per unit volume of a material." .

ec:DepartmentStore a owl:Class ;
    rdfs:label "Category D2 - department stores"@en ;
    dcterms:source "EN 1991-1-1:2002 Table 6.1" ;
    rdfs:subClassOf ec:ShoppingAreas ;
    skos:definition "Area in department stores" ;
    skos:example "department stores" .

ec:DesignAction a owl:Class ;
    rdfs:label "Design action"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.3.21" ;
    rdfs:subClassOf ec:Action ;
    owl:disjointWith ec:RepresentativeAction ;
    skos:definition "Value ofan action used for limit state verification. A design valueo is btained by multiplying the representative value by the partial factor." .

ec:DirectAction a owl:Class ;
    rdfs:label "Direct action"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.3.1 a)" ;
    rdfs:subClassOf ec:Action ;
    owl:disjointWith ec:IndirectAction ;
    skos:definition "Set of forces (loads) applied to the structure." .

ec:Displacement a owl:Class ;
    rdfs:label "Displacement"@en ;
    dcterms:source "EN 1990:2002, Section 1.6" ;
    rdfs:subClassOf ec:LinearDeformation ;
    skos:altLabel "u" ;
    skos:definition "Horizontal displacement of a structure or structural member." .

ec:EQU a owl:NamedIndividual,
        ec:UltimateLimitState ;
    rdfs:label "Static equilibrium"@en ;
    dcterms:source "EN 1990:2002, Section 6.4.1(1)P a)" ;
    skos:definition "Loss of static equilibrium of the structure or any part of it considered as a rigid body." .

ec:Execution a owl:NamedIndividual,
        ec:TransientDesignSituation ;
    rdfs:label "Execution"@en ;
    dcterms:source "EN 1990:2002, Section 3.2(2)P" ;
    skos:definition "Transient design situation during execution of a construction work." .

ec:ExplosionDesignSituation a owl:NamedIndividual,
        ec:AccidentalDesignSituation ;
    rdfs:label "Explosion design situation"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.2.5" ;
    skos:definition "Accidental design situation involving explosion conditions." .

ec:FAT a owl:NamedIndividual,
        ec:UltimateLimitState ;
    rdfs:label "Fatigue failure"@en ;
    dcterms:source "EN 1990:2002, Section 6.4.1(1)P d)" ;
    skos:definition "Fatigue failure of the structure or structural members." .

ec:FatigueAction a owl:Class ;
    rdfs:label "Fatigue action"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.3.7" ;
    rdfs:subClassOf ec:Action ;
    skos:definition "Repeated or fluctuating mechanical action applied over time, which may cause failure even if the individual stress cycles are below the material's static strength." .

ec:FatigueEffect a owl:Class ;
    rdfs:label "Fatigue effect"@en ;
    dcterms:source "EN 1990:2002, Section 3.1(5)" ;
    rdfs:subClassOf ec:TimeDependentEffect ;
    skos:definition "Progressive damage effect due to repeated loading cycles." .

ec:FavourableAction a owl:Class ;
    rdfs:label "Favourable action"@en ;
    dcterms:source "EN 1990:2002, Section 6.3.2(3)P" ;
    rdfs:subClassOf ec:Action ;
    owl:disjointWith ec:UnfavourableAction ;
    skos:definition "Action that reduces the effect of other actions on a structure, or otherwise contributes to structural safety." .

ec:FireDesignSituation a owl:NamedIndividual,
        ec:AccidentalDesignSituation ;
    rdfs:label "Fire design situation"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.2.5 & 6" ;
    skos:definition "Accidental design situation involving fire conditions requiring specific design considerations." .

ec:FixedAction a owl:Class ;
    rdfs:label "Fixed action"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.3.8" ;
    rdfs:subClassOf ec:Action ;
    owl:disjointWith ec:FreeAction ;
    skos:definition "Action that has a fixed distribution and position over the structure or structural member such that the magnitude and direction of the action are determined unambiguously for the whole structure." ;
    skos:example "Stored goods in a defined storage area with no alternative placement" .

ec:FundamentalCombination a owl:Class ;
    rdfs:label "Fundamental combination"@en ;
    dcterms:source "EN 1990:2002, Section 6.4.3.2" ;
    rdfs:subClassOf ec:ULSCombination ;
    owl:disjointWith ec:AccidentalCombination,
        ec:SeismicCombination ;
    skos:definition "Combination of actions for persistent or transient design situations." ;
    skos:scopeNote "Used for persistent and transient design situations in ULS verifications" .

ec:GEO a owl:NamedIndividual,
        ec:UltimateLimitState ;
    rdfs:label "Geotechnical failure"@en ;
    dcterms:source "EN 1990:2002, Section 6.4.1(1)P c)" ;
    skos:definition "Failure or excessive deformation of the ground where the strengths of soil or rock are significant in providing resistance." .

ec:GeneralRetailShops a owl:Class ;
    rdfs:label "Category D1 - general retail shops"@en ;
    dcterms:source "EN 1991-1-1:2002 Table 6.1" ;
    rdfs:subClassOf ec:ShoppingAreas ;
    skos:definition "Area in general retail shops" ;
    skos:example "general retail shops" .

ec:GeotechnicalAction a owl:Class ;
    rdfs:label "Geotechnical action"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.3.7" ;
    rdfs:subClassOf ec:Action ;
    skos:definition "Action transmitted to the structure by the ground, fill or groundwater." .

ec:HYD a owl:NamedIndividual,
        ec:UltimateLimitState ;
    rdfs:label "Hydraulic failure"@en ;
    dcterms:source "EN 1990:2002/A1:2005, Section 6.4.1(1)P f)" ;
    skos:definition "Hydraulic heave, internal erosion and piping in the ground caused by hydraulic gradients." .

ec:HelicopterLandingAreas a owl:Class ;
    rdfs:label "Category K - helicopter landing areas"@en ;
    dcterms:source "EN 1991-1-1:2002 Table 6.9" ;
    rdfs:subClassOf ec:EurocodeZone ;
    skos:definition "Roof accessible for special services, such as helicopter landing areas" ;
    skos:example "helicopter landing pads on roofs" .

ec:ISLS a owl:NamedIndividual,
        ec:ServiceabilityLimitState ;
    rdfs:label "Irreversible Serviceability Limit State"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.2.14.1" ;
    skos:definition "Serviceability limit state where some consequences of actions exceeding the specified service requirements will remain when the actions are removed." .

ec:ImpactDesignSituation a owl:NamedIndividual,
        ec:AccidentalDesignSituation ;
    rdfs:label "Impact design situation"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.2.5" ;
    skos:definition "Accidental design situation involving impact conditions." .

ec:IndustrialUse a owl:Class ;
    rdfs:label "Category E2 - industrial use"@en ;
    dcterms:source "EN 1991-1-1:2002 Table 6.3" ;
    rdfs:subClassOf ec:IndustrialandStorage ;
    skos:definition "Industrial use area" ;
    skos:example "industrial facilities: plant rooms, workshops" .

ec:LargeCrowdsAreas a owl:Class ;
    rdfs:label "Category C5 - large crowds areas"@en ;
    dcterms:source "EN 1991-1-1:2002 Table 6.1" ;
    rdfs:subClassOf ec:CongregationArea ;
    skos:definition "Area susceptible to large crowds" ;
    skos:example "buildings for public events like concert halls, sports halls including stands, terraces and access areas and railway platforms" .

ec:LeadingVariableAction a owl:Class ;
    rdfs:label "Leading variable action"@en ;
    dcterms:source "EN 1990:2002 Section 6.4.3.1(2)" ;
    rdfs:subClassOf ec:VariableAction ;
    owl:disjointWith ec:AccompanyingVariableAction ;
    skos:definition "Variable action that, in a particular combination of actions, has the most significant effect on the design outcome and is considered as the primary representative of the variable actions in that combination." .

ec:LightVehicleTraffic a owl:Class ;
    rdfs:label "Category F - light vehicle traffic"@en ;
    dcterms:source "EN 1991-1-1:2002 Table 6.7" ;
    rdfs:subClassOf ec:EurocodeZone ;
    skos:definition "Traffic and parking area for light vehicles (≤ 30 kN gross vehicle weight and ≤ 8 seats not including driver)" ;
    skos:example "garages, parking areas, parking halls" .

ec:LimitingServicabilityCriterion a owl:Class ;
    rdfs:label "Limiting serviceability criterion"@en ;
    dcterms:source "EN 1990:2002, Section 6.5.1" ;
    rdfs:subClassOf ec:LimitStateValue ;
    skos:definition "Limiting design value of the relevant serviceability criterion." ;
    skos:example "allowable deflection, allowable crack width" .

ec:LocalizedFailureDesignSituation a owl:NamedIndividual,
        ec:AccidentalDesignSituation ;
    rdfs:label "Localized failure design situation"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.2.5" ;
    skos:definition "Accidental design situation involving local failure conditions." .

ec:MediumVehicleTraffic a owl:Class ;
    rdfs:label "Category G - medium vehicle traffic"@en ;
    dcterms:source "EN 1991-1-1:2002 Table 6.7" ;
    rdfs:subClassOf ec:EurocodeZone ;
    skos:definition "Traffic and parking area for medium vehicles (>30 kN, ≤ 160 kN gross vehicle weight, on 2 axles)" ;
    skos:example "access routes, delivery zones, zones accessible to fire engines (≤ 160 kN gross vehicle weight)" .

ec:MonumentalBuldingStructure a owl:NamedIndividual,
        ec:Structure ;
    rdfs:label "Monumental building structure"@en ;
    dcterms:source "EN 1990:2002, Table 2.1" ;
    skos:definition "Large-scale, architecturally significant structure designed to serve as a landmark, memorial, or symbol of cultural, historical, or civic importance." ;
    skos:example "national parliment building, museum of significant importance, major cathedral" .

ec:NormalStress a owl:Class ;
    rdfs:label "Normal stress"@en ;
    rdfs:subClassOf ec:Stress ;
    skos:definition "Stress acting in the direction normal to a surface." .

ec:OfficeArea a owl:Class ;
    rdfs:label "Category B - office areas"@en ;
    dcterms:source "EN 1991-1-1:2002 Table 6.1" ;
    rdfs:subClassOf ec:EurocodeZone ;
    skos:definition "Office area" ;
    skos:example "General office spaces" .

ec:PersistentDesignSituation a owl:Class ;
    rdfs:label "Persistent design situation"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.2.4" ;
    rdfs:subClassOf ec:DesignSituation ;
    skos:definition "Design situation that is relevant during a period of the same order as the design working life of the structure. Generally refers to conditions of normal use." .

ec:PhysicalActivitiesAreas a owl:Class ;
    rdfs:label "Category C4 - physical activities areas"@en ;
    dcterms:source "EN 1991-1-1:2002 Table 6.1" ;
    rdfs:subClassOf ec:CongregationArea ;
    skos:definition "Area with possible physical activities" ;
    skos:example "dance halls, gymnastic rooms, stages" .

ec:PrincipalStress a owl:Class ;
    rdfs:label "Principal stress"@en ;
    rdfs:subClassOf ec:Stress ;
    skos:definition "Maximum or minimum normal stress at a point." .

ec:QuasiStaticAction a owl:Class ;
    rdfs:label "Quasi-static action"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.3.13" ;
    rdfs:subClassOf ec:DynamicAction ;
    skos:definition "Dynamic action represented by an equivalent static action in a static model." .

ec:RSLS a owl:NamedIndividual,
        ec:ServiceabilityLimitState ;
    rdfs:label "Reversible Serviceability Limit State"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.2.14.2" ;
    skos:definition "Serviceability limit state where no consequences of actions exceeding the specified service requirements will remain when the actions are removed." .

ec:Reaction a owl:Class ;
    rdfs:label "Reaction"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.3.2" ;
    rdfs:subClassOf ec:EffectOfAction ;
    skos:definition "Support reaction force or moment at structural supports." .

ec:Repair a owl:NamedIndividual,
        ec:TransientDesignSituation ;
    rdfs:label "Repair"@en ;
    dcterms:source "EN 1990:2002, Section 3.2(2)P" ;
    skos:definition "Transient design situation during repair of a construction work." .

ec:ReplacableStructuralElements a owl:NamedIndividual,
        ec:Structure ;
    rdfs:label "Structure with replaceable elements"@en ;
    dcterms:source "EN 1990:2002, Table 2.1" ;
    skos:definition "Structure designed such that certain primary or secondary structural components can be removed, replaced, or upgraded during its service life without compromising the overall stability, integrity, or usability of the structure." ;
    skos:example "modular steel bridge with replaceable girders, industrial building with precast concrete roof panels" .

ec:Residential a owl:Class ;
    rdfs:label "Category A - domestic and residential"@en ;
    dcterms:source "EN 1991-1-1:2002 Table 6.1" ;
    rdfs:subClassOf ec:EurocodeZone ;
    skos:definition "Area for domestic and residential activities" ;
    skos:example "rooms in residential buildings and houses, bedrooms and wards in hospitals, bedrooms in hotels and hostels, kitchens and toilets" .

ec:ResidentialBuilding a owl:Class ;
    rdfs:label "Residential building"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.1.2" ;
    rdfs:subClassOf ec:Building ;
    skos:definition "Building intended primarily for the accommodation and permanent or temporary residence of individuals or households." ;
    skos:example "detached house, apartment building, high-rise residential tower" .

ec:Roof a owl:Class ;
    rdfs:label "Category H - roofs"@en ;
    dcterms:source "EN 1991-1-1:2002 Table 6.9" ;
    rdfs:subClassOf ec:EurocodeZone ;
    skos:definition "Roof not accessible except for normal maintenance and repair" ;
    skos:example "roofs accessible only for maintenance" .

ec:Rotation a owl:Class ;
    rdfs:label "Rotation"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.3.2" ;
    rdfs:subClassOf ec:AngularDeformation ;
    skos:definition "Angular rotation of a structure or structural member." .

ec:STR a owl:NamedIndividual,
        ec:UltimateLimitState ;
    rdfs:label "Structural resistance"@en ;
    dcterms:source "EN 1990:2002, Section 6.4.1(1)P b)" ;
    skos:definition "Internal failure or excessive deformation of the structure or structural members where the strength of construction materials governs." .

ec:SeismicAction a owl:Class ;
    rdfs:label "Seismic action"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.3.6" ;
    rdfs:subClassOf ec:AccidentalAction ;
    skos:altLabel "A_E" ;
    skos:definition "Action that arises due to earthquake ground motions." .

ec:SeismicDesignSituation a owl:Class ;
    rdfs:label "Seismic design situation"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.2.7" ;
    rdfs:subClassOf ec:DesignSituation ;
    skos:definition "Design situation involving exceptional conditions of the structure when subjected to a seismic event." .

ec:SelfWeight a owl:Class ;
    rdfs:label "Self-weight"@en ;
    dcterms:source "EN 1991-1-1:2002" ;
    rdfs:subClassOf ec:PermanentAction ;
    skos:definition "Weight of the structure itself, including all permanent construction elements, finishes, fixed equipment, and any other permanently attached components." .

ec:ShearForce a owl:Class ;
    rdfs:label "Shear force"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.3.2" ;
    rdfs:subClassOf ec:InternalForce ;
    skos:altLabel "V" ;
    skos:definition "Internal force acting perpendicular to the axis of structural members." .

ec:ShearStress a owl:Class ;
    rdfs:label "Shear stress"@en ;
    rdfs:subClassOf ec:Stress ;
    skos:definition "Stress acting in the direction parallel (tangential) to a surface." .

ec:ShrinkageEffect a owl:Class ;
    rdfs:label "Shrinkage effect"@en ;
    rdfs:subClassOf ec:TimeDependentEffect ;
    skos:definition "Deformation effect due to material shrinkage over time." .

ec:StaticAction a owl:Class ;
    rdfs:label "Static action"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.3.11" ;
    rdfs:subClassOf ec:Action ;
    owl:disjointWith ec:DynamicAction ;
    skos:definition "Action that does not cause significant acceleration of the structure or structural members." .

ec:StorageAreas a owl:Class ;
    rdfs:label "Category E1 - storage areas"@en ;
    dcterms:source "EN 1991-1-1:2002 Table 6.3" ;
    rdfs:subClassOf ec:IndustrialandStorage ;
    skos:definition "Area susceptible to accumulation of goods, including access areas" ;
    skos:example "areas for storage use including storage of books and other documents: archives, libraries, stockrooms" .

ec:Strain a owl:Class ;
    rdfs:label "Strain"@en ;
    dcterms:source "EN 1990:2002, Section 1.5.3.2" ;
    rdfs:subClassOf ec:MechanicalEffect ;
    skos:definition "Deformation per unit length in structural members." .

ec:TemporaryStructure a owl:NamedIndividual,
        ec:Structure ;
    rdfs:label "Temporary structure"@en ;
    dcterms:source "EN 1990:2002, Table 2.1" ;
    skos:definition "Structre that is intended to be used for a limited period of time, typically to support, protect, or provide access during the execution of permanent works, or to serve a short-term purpose." ;
    skos:example "formwork system, steel scaffolding, temoprary bridge" .

ec:Thickness a owl:Class ;
