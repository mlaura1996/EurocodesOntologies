:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontology-of-units-of-measure.org/resource/om-2/hasUnit
om:hasUnit rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain mmo:Property ;
           rdfs:range om:Unit .


###  https://w3id.org/conprop#hasBinderQuantity
:hasBinderQuantity rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasComponentQuantity ;
                   rdfs:domain :BinderMaterial ;
                   rdfs:range :BinderQuantity .


###  https://w3id.org/conprop#hasChemicalComponent
:hasChemicalComponent rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:domain :BinderMaterial ;
                      rdfs:range mmo:Composition .


###  https://w3id.org/conprop#hasCoarseAggregateQuantity
:hasCoarseAggregateQuantity rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :hasComponentQuantity ;
                            rdfs:domain :CoarseAggregateMaterial ;
                            rdfs:range :CoarseAggregateQuantity .


###  https://w3id.org/conprop#hasComponentQuantity
:hasComponentQuantity rdf:type owl:ObjectProperty ;
                      rdfs:domain mat:Concrete ;
                      rdfs:range :ComponentQuantity .


###  https://w3id.org/conprop#hasFineAggregateQuantity
:hasFineAggregateQuantity rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :hasComponentQuantity ;
                          rdfs:domain :FineAggregateMaterial ;
                          rdfs:range :FineAggregateQuantity .


###  https://w3id.org/conprop#hasMaterialProperty
:hasMaterialProperty rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isMaterialPropertyOf ;
                     rdfs:domain mat:Material ;
                     rdfs:range mmo:MaterialProperty .


###  https://w3id.org/conprop#hasWaterQuantity
:hasWaterQuantity rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasComponentQuantity ;
                  rdfs:domain mat:Water ;
                  rdfs:range :WaterQuantity .


###  https://w3id.org/conprop#isComponentMaterialOf
:isComponentMaterialOf rdf:type owl:ObjectProperty ;
                       owl:inverseOf :isMadeOfComponentMaterial .


###  https://w3id.org/conprop#isMadeOfComponentMaterial
:isMadeOfComponentMaterial rdf:type owl:ObjectProperty ;
                           rdfs:domain mat:Concrete ;
                           rdfs:range :ConcreteComponentMaterial .


###  https://w3id.org/conprop#isMaterialPropertyOf
:isMaterialPropertyOf rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/conprop#AdditionMaterial
:AdditionMaterial rdf:type owl:Class ;
                  rdfs:subClassOf :BinderMaterial ;
                  owl:disjointWith :Cement ;
                  rdfs:label "addition" .


###  https://w3id.org/conprop#AdmixtureMaterial
:AdmixtureMaterial rdf:type owl:Class ;
                   rdfs:subClassOf :ConcreteComponentMaterial ;
                   rdfs:comment "https://www.wikidata.org/wiki/Q831619"^^xsd:anyURI ,
                                "Natural or manufactured chemical or additive added during concrete mixing to enhance specific properties of the fresh or hardened concrete." ;
                   rdfs:label "admixture" .


###  https://w3id.org/conprop#AggregateMaterial
:AggregateMaterial rdf:type owl:Class ;
                   rdfs:subClassOf :ConcreteComponentMaterial ;
                   rdfs:comment "https://www.wikidata.org/wiki/Q866298"^^xsd:anyURI ,
                                "Inert material with sufficient compressive strength and appropriate size distribution added as load to a composite." ;
                   rdfs:label "aggregate"@en .


###  https://w3id.org/conprop#BinderMaterial
:BinderMaterial rdf:type owl:Class ;
                rdfs:subClassOf :ConcreteComponentMaterial ;
                rdfs:comment "A material or substance that holds or draws other materials together to form a cohesive whole mechanically, chemically, by adhesion or cohesion. More narrowly, binders are liquid or dough-like substances that harden by a chemical or physical process and bind fibres, filler powder and other particles added into it."@en ;
                rdfs:label "binder" .


###  https://w3id.org/conprop#BinderQuantity
:BinderQuantity rdf:type owl:Class ;
                rdfs:subClassOf :ComponentQuantity .


###  https://w3id.org/conprop#Cement
:Cement rdf:type owl:Class ;
        rdfs:subClassOf :BinderMaterial ;
        rdfs:comment "A chemical substance used for construction that sets, hardens, and adheres to other materials to bind them together."@en ;
        rdfs:label "cement"@en .


###  https://w3id.org/conprop#CoarseAggregateMaterial
:CoarseAggregateMaterial rdf:type owl:Class ;
                         rdfs:subClassOf :AggregateMaterial ;
                         owl:disjointWith :FineAggregateMaterial ;
                         rdfs:comment "Aggregate characterised by the size which does not allow it to pass through a certain threshold sieve size in the sieve particle size distribution analysis." ;
                         rdfs:label "coarse aggregate" .


###  https://w3id.org/conprop#CoarseAggregateQuantity
:CoarseAggregateQuantity rdf:type owl:Class ;
                         rdfs:subClassOf :ComponentQuantity .


###  https://w3id.org/conprop#ComponentQuantity
:ComponentQuantity rdf:type owl:Class ;
                   rdfs:subClassOf mmo:Property ;
                   owl:disjointWith mmo:MaterialProperty ;
                   rdfs:label "concrete component quantity" .


###  https://w3id.org/conprop#ConcreteComponentMaterial
:ConcreteComponentMaterial rdf:type owl:Class ;
                           rdfs:subClassOf mat:Material ;
                           rdfs:comment "A consitituent of the fresh concrete mix, which usually encompasses water, aggregate (fine and coarse) and binder (cement and additions)." ;
                           rdfs:label "concrete component"@en .


###  https://w3id.org/conprop#CorrosionResistance
:CorrosionResistance rdf:type owl:Class ;
                     rdfs:subClassOf mmo:ChemicalProperty ;
                     owl:disjointWith mmo:Composition ;
                     rdfs:label "corrosion resistance" .


###  https://w3id.org/conprop#Creep
:Creep rdf:type owl:Class ;
       rdfs:subClassOf mmo:MechanicalProperty ;
       rdfs:label "creep"@en .


###  https://w3id.org/conprop#Emissivity
:Emissivity rdf:type owl:Class ;
            rdfs:subClassOf mmo:ThermalProperty ;
            rdfs:label "emissivity" .


###  https://w3id.org/conprop#Extrudability
:Extrudability rdf:type owl:Class ;
               rdfs:subClassOf :ManufacturingProperty ;
               rdfs:label "extrudability" .


###  https://w3id.org/conprop#FineAggregateMaterial
:FineAggregateMaterial rdf:type owl:Class ;
                       rdfs:subClassOf :AggregateMaterial ;
                       rdfs:comment "Aggregate characterised by the size which allows it to pass through a certain threshold sieve size in the sieve particle size distribution analysis." ;
                       rdfs:label "fine aggregate" .


###  https://w3id.org/conprop#FineAggregateQuantity
:FineAggregateQuantity rdf:type owl:Class ;
                       rdfs:subClassOf :ComponentQuantity .


###  https://w3id.org/conprop#Flowability
:Flowability rdf:type owl:Class ;
             rdfs:subClassOf :ManufacturingProperty ;
             rdfs:label "flowability" .


###  https://w3id.org/conprop#HydrationHeat
:HydrationHeat rdf:type owl:Class ;
               rdfs:subClassOf mmo:ThermalProperty ;
               rdfs:label "cumulative unit hydration heat"@en .


###  https://w3id.org/conprop#ManufacturingProperty
:ManufacturingProperty rdf:type owl:Class ;
                       rdfs:subClassOf mmo:MaterialProperty ;
                       rdfs:label "manufacturing property" .


###  https://w3id.org/conprop#OpenTime
:OpenTime rdf:type owl:Class ;
          rdfs:subClassOf :ManufacturingProperty ;
          rdfs:label "open time" .


###  https://w3id.org/conprop#Pumpability
:Pumpability rdf:type owl:Class ;
             rdfs:subClassOf :ManufacturingProperty ;
             rdfs:label "pumpability" .


###  https://w3id.org/conprop#WaterQuantity
:WaterQuantity rdf:type owl:Class ;
               rdfs:subClassOf :ComponentQuantity .


###  https://w3id.org/conprop#Workability
:Workability rdf:type owl:Class ;
             rdfs:subClassOf :ManufacturingProperty ;
             rdfs:label "workability" .

#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( mat:Water
                :AdmixtureMaterial
                :AggregateMaterial
                :BinderMaterial
              )
] .


