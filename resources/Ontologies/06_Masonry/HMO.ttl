                  rdfs:label "HorizontalJoints"@IRI-based; 
                  rdfs:label "Horizontal Joints"@en . 
#  --------------------------- Class  15------------------------- 
:ShearStrengthTC rdf:type owl:Class; 
                 rdfs:comment "Average shear strength according to the Turnsek and Cacovic criterion. It is used in the case of irregular-textured masonry and corresponds to diagonal tensile failure at the masonry level." ;
                 owl:disjointWith :TensileStrength ;
                 owl:disjointWith :ShearStrengthMC ;
                 owl:disjointWith :CompressiveStrength ;
                 owl:disjointWith :YoungModulus ;
                 owl:disjointWith :MassDensity ;
                 owl:disjointWith :ShearModulus ;
                 rdfs:subClassOf :HomogenisedMechanicalProperty ;
                 rdfs:label "ShearStrengthTC"@IRI-based; 
                 rdfs:label "Shear Strength (Turnsek and Cacovic)"@en . 
