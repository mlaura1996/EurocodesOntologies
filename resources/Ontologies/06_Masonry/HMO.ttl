                               owl:disjointWith :PatternEntities ;
                               owl:disjointWith :Units ;
                               owl:disjointWith :MasonryLayer ;
                               owl:disjointWith :MasonryElement ;
                               owl:disjointWith :Pattern ;
                               owl:disjointWith :RepresentativeVolumeElement ;
                               rdfs:label "HomogenisedMechanicalProperty"@IRI-based; 
                               rdfs:label "Homogenised Mechanical Property"@en . 
